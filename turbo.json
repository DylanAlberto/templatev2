{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "cache": true,
      "dependsOn": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}



