{
  "name": "templatev2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "concurrently -n \"supabase,web\" -c \"blue,magenta\" \"pnpm exec supabase start\" \"turbo run dev\"",
    "dev:web": "turbo run dev",
    "dev:supabase": "pnpm exec supabase start",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,tsx,md,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,tsx,md,json,yml,yaml}\"",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.0.0",
    "supabase": "^2.58.5",
    "turbo": "^2.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.47.0"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
